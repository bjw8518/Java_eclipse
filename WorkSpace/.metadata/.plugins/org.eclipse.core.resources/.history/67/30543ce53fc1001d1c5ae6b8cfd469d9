
public class Java1_Loop {

	public static void main(String[] args) {
		//반복문 for
		/*for (초기값; 종료조건; 증가분;){
		 * System.out.println("i");
		 * for(int i = 0; i<10; i++) {
			System.out.println("i");
		}
		*/
		int a = 0;
		System.out.println(a);
		a = a - 100; //대입입니당
		System.out.println(a);
		
		
		}

	}
