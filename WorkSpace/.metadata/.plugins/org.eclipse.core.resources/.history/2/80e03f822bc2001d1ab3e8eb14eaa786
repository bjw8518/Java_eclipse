
public class loop {

	public static void main(String[] args) {
		
//		for(int a=0; a <100; a++) {
//			System.out.println(a+"번쨰");
//			System.out.println(a+1);
//		}
		int sumsum =0;
		for(int i = 0;i < 100;i++) {
			sumsum = sumsum + (i+1);
			//1 <= 0 + 1;
			//3 <= 1 + 2;
			//
		}
		System.out.println(sumsum);
		
		int sum1 = 0;
		for(int a=0;a<1000;a++) {
			sum1 = sum1+ a+1;
			System.out.println(sum1);
		}
	}

}
