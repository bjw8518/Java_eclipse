package p23;

import java.util.Random;
import java.util.Scanner;

public class RandomEx {

	public static void main(String[] args) {
		//2로 나눴을때 0이 나오면 짝수
		//1이 나오면 홀수
		
		
		Random r = new Random();
		int randomValue = r.nextInt(100);
		System.out.println(randomValue);
		int result = randomValue / 2;
	}

}
