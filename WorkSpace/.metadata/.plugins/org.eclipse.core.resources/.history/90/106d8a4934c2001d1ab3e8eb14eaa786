import java.util.Scanner;

public class P40 {

	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		for(int i = 0; i<999999999;i++) {
			System.out.println("다음 중 원하는메뉴 입력");
			System.out.println("1. Hello 출력");
			System.out.println("q. 종료");
			
			String im = s.nextLine();//문자열을 받을때는 nextLine()사용
			//만약 입력받은 문자열을 숫자로 변경하고싶은경우 아래 부분 사용
			//int number = Integer.parseInt(im);
			
			if(im.equals("1")) {
				System.out.println("Hello");
			}else if (im.equals("q")) {
				break;
			}else {
				System.out.println("입력값 잘못입력");
			}
			
			
			
		}

	}

}
