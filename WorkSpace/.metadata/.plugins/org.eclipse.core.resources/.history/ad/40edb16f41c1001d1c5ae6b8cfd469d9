
public class Java1_Loop {

	public static void main(String[] args) {
		//반복문 for
		//int i = 0; //지역변수
		//for (초기값; 종료조건; 증가분;){
		 for(int i = 0; i<=5; i++) { //i<=5(5를 포함한다), i<5 (5를 포함하지 않는다)
			System.out.println(i);
		}
		/*int a = 0;
		System.out.println(a);
		a++;
		//a +=1; // a = a + 1;
		a += 1;
		//a++ = a = a + 1; 같은내용
		System.out.println(a);
